{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "catalyzer",
  "version": "0.1.0",
  "identifier": "com.tauri.catalyzer",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "width": 600,
        "resizable": false,
        "title": "Catalyzer",
        "label": "main",
        "visible": false
      },
      {
        "width": 400,
        "height": 200,
        "decorations": false,
        "url": "webviews/splashscreen",
        "label": "splashscreen"
      }
